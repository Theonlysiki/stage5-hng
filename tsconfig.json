{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es6"],
    "moduleResolution": "node",
    "module": "commonjs",
    "rootDir": "src",
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "types": ["node", "express"],
    "typeRoots" : ["./src/types/", "./node_modules/@types"]
  },
}
